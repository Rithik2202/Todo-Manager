<table>
    <% for(var i=0; i< allTodos.length; i++){%>
    <tr>
        <td>
            <%=allTodos[i].title %>
        </td>
        <td>
            <%=allTodos[i].dueDate %>
        </td>
        <td>
            <%=allTodos[i].completed %>
        </td>
        <td>
            <%=allTodos[i].createdAt %>
        </td>
        <td>
            <%=allTodos[i].updatedAt %>
        </td>
    </tr>
<% } %>
</table>